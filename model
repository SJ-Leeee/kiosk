npx sequelize model:generate --name Item --attributes name:string,price:integer,type:enum,amount:integer
npx sequelize model:generate --name Option --attributes item_id:integer,option_name:string,option_price:integer
npx sequelize model:generate --name Order_item --attributes item_id:integer,state:enum,amount:integer
npx sequelize model:generate --name Cart --attributes user_id:integer,state:boolean
npx sequelize model:generate --name Cart_detail_option --attributes item_id:integer,cart_id:integer,option_id:integer
